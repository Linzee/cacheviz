                  "tooltip": {
                    "signal": "{\"x\": format(datum[\"x\"], \"\"), \"y\": format(datum[\"y\"], \"\")}"
                  },
                  "x": {"scale": "concat_0_concat_0_x", "field": "x"},
                  "y": {"scale": "concat_0_concat_0_y", "field": "y"}
                }
              }
