{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "description": "CacheViz",
  "autosize": "pad",
  "padding": 5,
  "data": [
    {"name": "cache_store"},
    {"name": "type_store"},
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/Linzee/cacheviz/master/caches.csv",
      "format": {"type": "csv"}
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [{"type": "identifier", "as": "_vgsid_"}]
    },
    {
      "name": "data_1",
      "source": "data_0",
      "transform": [
        {
          "type": "filter",
          "expr": "datum[\"Origin\"] !== null && !isNaN(datum[\"Origin\"]) && datum[\"Cylinders\"] !== null && !isNaN(datum[\"Cylinders\"])"
        }
      ]
    },
    {
      "name": "data_2",
      "source": "data_0",
      "transform": [
        {
          "type": "filter",
          "expr": "!(length(data(\"type_store\"))) || (vlSelectionTest(\"type_store\", datum))"
        },
        {
          "type": "filter",
          "expr": "datum[\"x\"] !== null && !isNaN(datum[\"x\"]) && datum[\"y\"] !== null && !isNaN(datum[\"y\"])"
        }
      ]
    },
    {
      "name": "data_3",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "!(length(data(\"cache_store\"))) || (vlSelectionTest(\"cache_store\", datum))"
        }
      ]
    },
    {
      "name": "data_4",
      "source": "data_3",
      "transform": [
        {"type": "identifier", "as": "_vgsid_"},
        {
          "type": "aggregate",
          "groupby": ["type"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        },
        {"type": "identifier", "as": "_vgsid_"}
      ]
    },
    {
      "name": "data_5",
      "source": "data_3",
      "transform": [
        {
          "type": "formula",
          "expr": "datum[\"size\"]===\"micro\" ? 0 : datum[\"size\"]===\"small\" ? 1 : datum[\"size\"]===\"regular\" ? 2 : datum[\"size\"]===\"large\" ? 3 : datum[\"size\"]===\"virtual\" ? 4 : datum[\"size\"]===\"other\" ? 5 : datum[\"size\"]===\"not_chosen\" ? 6 : 7",
          "as": "x_size_sort_index"
        }
      ]
    },
    {
      "name": "data_6",
      "source": "data_5",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["size"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        }
      ]
    },
    {
      "name": "data_7",
      "source": "data_3",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["difficulty"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        }
      ]
    },
    {
      "name": "data_8",
      "source": "data_3",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["terrain"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        }
      ]
    }
  ],
  "signals": [
    {"name": "concat_0_concat_0_width", "value": 800},
    {"name": "concat_0_concat_0_height", "value": 800},
    {"name": "concat_0_concat_1_width", "value": 300},
    {"name": "concat_0_concat_1_concat_0_height", "value": 100},
    {"name": "concat_0_concat_1_concat_1_height", "value": 100},
    {"name": "concat_0_concat_1_concat_2_height", "value": 100},
    {"name": "concat_0_concat_1_concat_3_height", "value": 100},
    {"name": "concat_1_x_step", "value": 20},
    {
      "name": "concat_1_width",
      "update": "bandspace(domain('concat_1_x').length, 1, 0.5) * concat_1_x_step"
    },
    {"name": "concat_1_y_step", "value": 20},
    {
      "name": "concat_1_height",
      "update": "bandspace(domain('concat_1_y').length, 1, 0.5) * concat_1_y_step"
    },
    {
      "name": "unit",
      "value": {},
      "on": [
        {"events": "mousemove", "update": "isTuple(group()) ? group() : unit"}
      ]
    },
    {"name": "cache", "update": "vlSelectionResolve(\"cache_store\")"},
    {"name": "type", "update": "vlSelectionResolve(\"type_store\")"}
  ],
  "layout": {"padding": 20, "columns": 1, "bounds": "full", "align": "each"},
  "marks": [
