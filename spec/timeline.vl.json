{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "name": "timeline",
  "description": "CacheViz timeline",
  "data": {
    "values": []
  },
  "width": 800,
  "height": 100,
  "bounds": "full",
  "selection": {
    "range": {
      "type": "interval",
      "encodings": ["x"]
    }
  },
  "mark": "area",
  "encoding": {
    "x": {
      "type": "temporal",
      "field": "date",
      "timeUnit": "yearmonth",
      "axis": {
        "domain": false,
        "format": "%Y",
        "tickSize": 0
      }
    },
    "y": {
      "aggregate": "sum",
      "field": "value",
      "type": "quantitative",
      "axis": null,
      "stack": "center"
    }
  }
}
