{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "description": "CacheViz filters",
  "autosize": "pad",
  "padding": 5,
  "width": 300,
  "data": [
    {"name": "type_store"},
    {"name": "size_store"},
    {"name": "difficulty_store"},
    {"name": "terrain_store"},
    {"name": "source_0", "values": []},
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [{"type": "identifier", "as": "_vgsid_"}]
    },
    {
      "name": "data_1",
      "source": "data_0",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["type"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        },
        {"type": "identifier", "as": "_vgsid_"}
      ]
    },
    {
      "name": "data_2",
      "source": "data_0",
      "transform": [
        {
          "type": "formula",
          "expr": "datum[\"size\"]===\"micro\" ? 0 : datum[\"size\"]===\"small\" ? 1 : datum[\"size\"]===\"regular\" ? 2 : datum[\"size\"]===\"large\" ? 3 : datum[\"size\"]===\"virtual\" ? 4 : datum[\"size\"]===\"other\" ? 5 : datum[\"size\"]===\"not_chosen\" ? 6 : 7",
          "as": "x_size_sort_index"
        }
      ]
    },
    {
      "name": "data_3",
      "source": "data_2",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["size"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        },
        {"type": "identifier", "as": "_vgsid_"}
      ]
    },
    {
      "name": "data_4",
      "source": "data_0",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["difficulty"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        },
        {"type": "identifier", "as": "_vgsid_"}
      ]
    },
    {
      "name": "data_5",
      "source": "data_0",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["terrain"],
          "ops": ["count"],
          "fields": [null],
          "as": ["__count"]
        },
        {"type": "identifier", "as": "_vgsid_"}
      ]
    }
  ],
  "signals": [
    {"name": "concat_0_height", "value": 100},
    {"name": "concat_1_height", "value": 100},
    {"name": "concat_2_height", "value": 100},
    {"name": "concat_3_height", "value": 100},
    {
      "name": "unit",
      "value": {},
      "on": [
        {"events": "mousemove", "update": "isTuple(group()) ? group() : unit"}
      ]
    },
    {"name": "type", "update": "vlSelectionResolve(\"type_store\")"},
    {"name": "size", "update": "vlSelectionResolve(\"size_store\")"},
    {
      "name": "difficulty",
      "update": "vlSelectionResolve(\"difficulty_store\")"
    },
    {"name": "terrain", "update": "vlSelectionResolve(\"terrain_store\")"}
  ],
  "layout": {"padding": 20, "columns": 1, "bounds": "full", "align": "each"},
  "marks": [
    {
      "type": "group",
      "name": "concat_0_group",
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "width"},
          "height": {"signal": "concat_0_height"}
        }
      },
      "signals": [
        {
          "name": "type_tuple",
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "datum && item().mark.marktype !== 'group' ? {datum: datum, unit: \"concat_0\", fields: type_tuple_fields, values: [(item().isVoronoi ? datum.datum : datum)[\"_vgsid_\"]]} : null",
              "force": true
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "null"
            }
          ]
        },
        {
          "name": "type_tuple_fields",
          "value": [{"type": "E", "field": "_vgsid_"}]
        },
        {
          "name": "type_toggle",
          "value": false,
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "event.shiftKey"
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "false"
            }
          ]
        },
        {
          "name": "type_modify",
          "update": "modify(\"type_store\", type_toggle ? null : type_tuple, type_toggle ? null : true, type_toggle ? type_tuple : null)"
        }
      ],
      "marks": [
        {
          "name": "concat_0_marks",
          "type": "rect",
          "style": ["bar"],
          "from": {"data": "data_1"},
          "encode": {
            "update": {
              "fill": [
                {
                  "test": "!(length(data(\"terrain_store\"))) || (vlSelectionTest(\"terrain_store\", datum))",
                  "value": "#4c78a8"
                },
                {"value": "magenta"}
              ],
              "tooltip": {
                "signal": "{\"type\": ''+datum[\"type\"], \"Count of Records\": format(datum[\"__count\"], \"\")}"
              },
              "x": {"scale": "concat_0_x", "field": "type"},
              "width": {"scale": "concat_0_x", "band": true},
              "y": {"scale": "concat_0_y", "field": "__count"},
              "y2": {"scale": "concat_0_y", "value": 0}
            }
          }
        }
      ],
      "axes": [
        {
          "scale": "concat_0_x",
          "orient": "bottom",
          "grid": false,
          "title": "type",
          "labelAlign": "right",
          "labelAngle": 270,
          "labelBaseline": "middle",
          "labelOverlap": true,
          "zindex": 1
        },
        {
          "scale": "concat_0_y",
          "orient": "left",
          "grid": false,
          "title": "Count of Records",
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(concat_0_height/40)"},
          "zindex": 1
        },
        {
          "scale": "concat_0_y",
          "orient": "left",
          "gridScale": "concat_0_x",
          "grid": true,
          "tickCount": {"signal": "ceil(concat_0_height/40)"},
          "domain": false,
          "labels": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        }
      ]
    },
    {
      "type": "group",
      "name": "concat_1_group",
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "width"},
          "height": {"signal": "concat_1_height"}
        }
      },
      "signals": [
        {
          "name": "size_tuple",
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "datum && item().mark.marktype !== 'group' ? {datum: datum, unit: \"concat_1\", fields: size_tuple_fields, values: [(item().isVoronoi ? datum.datum : datum)[\"_vgsid_\"]]} : null",
              "force": true
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "null"
            }
          ]
        },
        {
          "name": "size_tuple_fields",
          "value": [{"type": "E", "field": "_vgsid_"}]
        },
        {
          "name": "size_toggle",
          "value": false,
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "event.shiftKey"
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "false"
            }
          ]
        },
        {
          "name": "size_modify",
          "update": "modify(\"size_store\", size_toggle ? null : size_tuple, size_toggle ? null : true, size_toggle ? size_tuple : null)"
        }
      ],
      "marks": [
        {
          "name": "concat_1_marks",
          "type": "rect",
          "style": ["bar"],
          "from": {"data": "data_3"},
          "encode": {
            "update": {
              "fill": [
                {
                  "test": "!(length(data(\"size_store\"))) || (vlSelectionTest(\"size_store\", datum))",
                  "value": "#4c78a8"
                },
                {"value": "magenta"}
              ],
              "tooltip": {
                "signal": "{\"size\": ''+datum[\"size\"], \"Count of Records\": format(datum[\"__count\"], \"\")}"
              },
              "x": {"scale": "concat_1_x", "field": "size"},
              "width": {"scale": "concat_1_x", "band": true},
              "y": {"scale": "concat_1_y", "field": "__count"},
              "y2": {"scale": "concat_1_y", "value": 0}
            }
          }
        }
      ],
      "axes": [
        {
          "scale": "concat_1_x",
          "orient": "bottom",
          "grid": false,
          "title": "size",
          "labelAlign": "right",
          "labelAngle": 270,
          "labelBaseline": "middle",
          "labelOverlap": true,
          "zindex": 1
        },
        {
          "scale": "concat_1_y",
          "orient": "left",
          "grid": false,
          "title": "Count of Records",
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(concat_1_height/40)"},
          "zindex": 1
        },
        {
          "scale": "concat_1_y",
          "orient": "left",
          "gridScale": "concat_1_x",
          "grid": true,
          "tickCount": {"signal": "ceil(concat_1_height/40)"},
          "domain": false,
          "labels": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        }
      ]
    },
    {
      "type": "group",
      "name": "concat_2_group",
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "width"},
          "height": {"signal": "concat_2_height"}
        }
      },
      "signals": [
        {
          "name": "difficulty_tuple",
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "datum && item().mark.marktype !== 'group' ? {datum: datum, unit: \"concat_2\", fields: difficulty_tuple_fields, values: [(item().isVoronoi ? datum.datum : datum)[\"_vgsid_\"]]} : null",
              "force": true
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "null"
            }
          ]
        },
        {
          "name": "difficulty_tuple_fields",
          "value": [{"type": "E", "field": "_vgsid_"}]
        },
        {
          "name": "difficulty_toggle",
          "value": false,
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "event.shiftKey"
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "false"
            }
          ]
        },
        {
          "name": "difficulty_modify",
          "update": "modify(\"difficulty_store\", difficulty_toggle ? null : difficulty_tuple, difficulty_toggle ? null : true, difficulty_toggle ? difficulty_tuple : null)"
        }
      ],
      "marks": [
        {
          "name": "concat_2_marks",
          "type": "rect",
          "style": ["bar"],
          "from": {"data": "data_4"},
          "encode": {
            "update": {
              "fill": [
                {
                  "test": "!(length(data(\"terrain_store\"))) || (vlSelectionTest(\"terrain_store\", datum))",
                  "value": "#4c78a8"
                },
                {"value": "magenta"}
              ],
              "tooltip": {
                "signal": "{\"difficulty\": ''+datum[\"difficulty\"], \"Count of Records\": format(datum[\"__count\"], \"\")}"
              },
              "x": {"scale": "concat_2_x", "field": "difficulty"},
              "width": {"scale": "concat_2_x", "band": true},
              "y": {"scale": "concat_2_y", "field": "__count"},
              "y2": {"scale": "concat_2_y", "value": 0}
            }
          }
        }
      ],
      "axes": [
        {
          "scale": "concat_2_x",
          "orient": "bottom",
          "grid": false,
          "title": "difficulty",
          "labelAlign": "right",
          "labelAngle": 270,
          "labelBaseline": "middle",
          "labelOverlap": true,
          "zindex": 1
        },
        {
          "scale": "concat_2_y",
          "orient": "left",
          "grid": false,
          "title": "Count of Records",
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(concat_2_height/40)"},
          "zindex": 1
        },
        {
          "scale": "concat_2_y",
          "orient": "left",
          "gridScale": "concat_2_x",
          "grid": true,
          "tickCount": {"signal": "ceil(concat_2_height/40)"},
          "domain": false,
          "labels": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        }
      ]
    },
    {
      "type": "group",
      "name": "concat_3_group",
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "width"},
          "height": {"signal": "concat_3_height"}
        }
      },
      "signals": [
        {
          "name": "terrain_tuple",
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "datum && item().mark.marktype !== 'group' ? {datum: datum, unit: \"concat_3\", fields: terrain_tuple_fields, values: [(item().isVoronoi ? datum.datum : datum)[\"_vgsid_\"]]} : null",
              "force": true
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "null"
            }
          ]
        },
        {
          "name": "terrain_tuple_fields",
          "value": [{"type": "E", "field": "_vgsid_"}]
        },
        {
          "name": "terrain_toggle",
          "value": false,
          "on": [
            {
              "events": [{"source": "scope", "type": "click"}],
              "update": "event.shiftKey"
            },
            {
              "events": [{"source": "scope", "type": "dblclick"}],
              "update": "false"
            }
          ]
        },
        {
          "name": "terrain_modify",
          "update": "modify(\"terrain_store\", terrain_toggle ? null : terrain_tuple, terrain_toggle ? null : true, terrain_toggle ? terrain_tuple : null)"
        }
      ],
      "marks": [
        {
          "name": "concat_3_marks",
          "type": "rect",
          "style": ["bar"],
          "from": {"data": "data_5"},
          "encode": {
            "update": {
              "fill": [
                {
                  "test": "!(length(data(\"terrain_store\"))) || (vlSelectionTest(\"terrain_store\", datum))",
                  "value": "#4c78a8"
                },
                {"value": "magenta"}
              ],
              "tooltip": {
                "signal": "{\"terrain\": ''+datum[\"terrain\"], \"Count of Records\": format(datum[\"__count\"], \"\")}"
              },
              "x": {"scale": "concat_3_x", "field": "terrain"},
              "width": {"scale": "concat_3_x", "band": true},
              "y": {"scale": "concat_3_y", "field": "__count"},
              "y2": {"scale": "concat_3_y", "value": 0}
            }
          }
        }
      ],
      "axes": [
        {
          "scale": "concat_3_x",
          "orient": "bottom",
          "grid": false,
          "title": "terrain",
          "labelAlign": "right",
          "labelAngle": 270,
          "labelBaseline": "middle",
          "labelOverlap": true,
          "zindex": 1
        },
        {
          "scale": "concat_3_y",
          "orient": "left",
          "grid": false,
          "title": "Count of Records",
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(concat_3_height/40)"},
          "zindex": 1
        },
        {
          "scale": "concat_3_y",
          "orient": "left",
          "gridScale": "concat_3_x",
          "grid": true,
          "tickCount": {"signal": "ceil(concat_3_height/40)"},
          "domain": false,
          "labels": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "concat_0_x",
      "type": "band",
      "domain": {"data": "data_1", "field": "type", "sort": true},
      "range": [0, {"signal": "width"}],
      "paddingInner": 0.1,
      "paddingOuter": 0.05
    },
    {
      "name": "concat_0_y",
      "type": "linear",
      "domain": {"data": "data_1", "field": "__count"},
      "range": [{"signal": "concat_0_height"}, 0],
      "nice": true,
      "zero": true
    },
    {
      "name": "concat_1_x",
      "type": "band",
      "domain": {
        "data": "data_2",
        "field": "size",
        "sort": {"op": "min", "field": "x_size_sort_index"}
      },
      "range": [0, {"signal": "width"}],
      "paddingInner": 0.1,
      "paddingOuter": 0.05
    },
    {
      "name": "concat_1_y",
      "type": "linear",
      "domain": {"data": "data_3", "field": "__count"},
      "range": [{"signal": "concat_1_height"}, 0],
      "nice": true,
      "zero": true
    },
    {
      "name": "concat_2_x",
      "type": "band",
      "domain": {"data": "data_4", "field": "difficulty", "sort": true},
      "range": [0, {"signal": "width"}],
      "paddingInner": 0.1,
      "paddingOuter": 0.05
    },
    {
      "name": "concat_2_y",
      "type": "linear",
      "domain": {"data": "data_4", "field": "__count"},
      "range": [{"signal": "concat_2_height"}, 0],
      "nice": true,
      "zero": true
    },
    {
      "name": "concat_3_x",
      "type": "band",
      "domain": {"data": "data_5", "field": "terrain", "sort": true},
      "range": [0, {"signal": "width"}],
      "paddingInner": 0.1,
      "paddingOuter": 0.05
    },
    {
      "name": "concat_3_y",
      "type": "linear",
      "domain": {"data": "data_5", "field": "__count"},
      "range": [{"signal": "concat_3_height"}, 0],
      "nice": true,
      "zero": true
    }
  ]
}
