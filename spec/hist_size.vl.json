{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "description": "CacheViz size histogram",
  "data": {
    "values": []
  },
  "width": 300,
  "height": 100,
  "mark": "bar",
  "selection": {
    "size": {
      "type": "multi",
      "resolve": "global"
    }
  },
  "encoding": {
    "x": {
      "field": "size",
      "type": "ordinal",
      "sort": [
        "micro",
        "small",
        "regular",
        "large",
        "virtual",
        "other",
        "not_chosen"
      ]
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative"
    }
  }
}
